{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run DEV App (Debug)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": [
        "--dart-define-from-file=config/development.json",
      ],
      "env": {}
    },
    {
      "name": "Run DEV Web Server (Debug)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": [
        "--dart-define-from-file=config/development.json",
        // "--device-id=web-server",
        "--device-id=chrome",
      ],
      "env": {}
    },
    {
      "name": "Run on macOS (Debug)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": [
        "--dart-define-from-file=config/development.json",
        "--device-id=macOS",
      ],
      "env": {}
    }
  ]
}